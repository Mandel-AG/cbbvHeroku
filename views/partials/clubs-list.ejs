<% clubs.forEach((club) => {%>
   <ul>
           <li id='idElement' class='list-group-item active'><%= club._id %></li> 
           <li class='list-group-item'>Name :<%= club.name %></li> 
           <li class='list-group-item'>introduction :<%= club.introduction %></li> 
           <li class='list-group-item' > Picture : <img width='100px' src="<%= club.picture %>" /> </li>
           <li class='list-group-item'>index :<%= club.index %></li> 
           <li class='list-group-item'>Time :<%= club.createdAt %></li> 
           <button id='delete' type='button' class="btn btn-danger w-25" clubid= <%- club._id %> >Delete</button>

   </ul>
   <% }) %>