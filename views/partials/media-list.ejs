<% medias.forEach((media) => {%>
    <ul>
            <li id='idElement' class='list-group-item active'><%= media._id %></li> 
            <li class='list-group-item'>Name :<%= media.name %></li> 
            <li class='list-group-item'>MediaType :<%= media.mediaType %></li> 
            <li class='list-group-item'>Team :<%= media.team %></li> 
            <li class='list-group-item'>Description : <%= media.description %></li> 
            <li class='list-group-item' > Picture : <img width='100px' src="<%= media.picture %>" /> </li>
            <!-- <% if(media.post) { %>
                <li class='list-group-item'>Post Type : <%= media.post.typePost %></li> 
                <li class='list-group-item'>Post title : <%= media.post.title %></li> 
            <% } %> -->
            <li class='list-group-item'>Time :<%= media.createdAt %></li> 
            <li class='list-group-item'>Index :<%= media.index %></li> 
            <button id='delete' type='button' class="btn btn-danger w-25" mediaid= <%- media._id %> >Delete</button>

    </ul>
    <% }) %>